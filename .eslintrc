{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2, "never"],
    "function-paren-newline": 0,
    "no-return-assign": 0,
    "react/jsx-filename-extension": 0,
    "react/no-multi-comp": 0,
    "react/prefer-stateless-function": 0
  }
}
